{
  "name": "angular-electron",
  "version": "4.1.0",
  "description": "Angular 6 with Electron (Typescript + SASS + Hot Reload)",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Maxime GRIS",
    "email": "maxime.gris@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "src/main/build/index.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "cross-env TARGET=web node postinstall",
    "postinstall:electron": "cross-env TARGET=electron-renderer node postinstall",
    "ng": "ng",
    "start:dev": "npm run start --env=dev",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve --aot",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:start": "npm run electron:tsc && electron .",
    "electron:tsc": "tsc -p src/main/tsconfig.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@ngrx/effects": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "application-config-path": "^0.1.0",
    "cheerio": "^1.0.0-rc.2",
    "crypto-js": "^3.1.9-1",
    "debug": "^3.1.0",
    "electron-is-dev": "^0.3.0",
    "fs-extra": "^7.0.0",
    "inversify": "^4.13.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "reflect-metadata": "^0.1.12",
    "rxjs-compat": "^6.2.2",
    "slugify": "^1.3.0",
    "xml-js": "^1.6.7",
    "snyk": "^1.90.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "6.0.5",
    "@angular/common": "6.0.5",
    "@angular/compiler": "6.0.5",
    "@angular/compiler-cli": "6.0.5",
    "@angular/core": "6.0.5",
    "@angular/forms": "6.0.5",
    "@angular/http": "6.0.5",
    "@angular/language-service": "6.0.5",
    "@angular/platform-browser": "6.0.5",
    "@angular/platform-browser-dynamic": "6.0.5",
    "@angular/router": "6.0.5",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/cheerio": "^0.22.8",
    "@types/crypto-js": "^3.1.43",
    "@types/fs-extra": "^5.0.4",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "^4.14.113",
    "@types/moment": "^2.13.0",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "core-js": "2.5.6",
    "cross-env": "^5.2.0",
    "electron": "2.0.8",
    "electron-builder": "20.16.4",
    "electron-reload": "1.2.5",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "npm-run-all": "4.1.3",
    "npx": "10.2.0",
    "protractor": "5.4.0",
    "rxjs": "^6.2.2",
    "ts-node": "6.0.3",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.10.0",
    "typescript": "^2.7.2",
    "wait-on": "2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.26"
  },
  "snyk": true
}
