{
  "name": "venobo-app",
  "productName": "Venobo",
  "version": "0.0.4",
  "description": "Your own free personal streaming application",
  "main": "./build",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "postinstall": "concurrently \"npm run build-renderer\" \"npm run build-dlls\" \"electron-builder install-app-deps\"",
    "build-dll": "better-npm-run build-dll",
    "build-main": "better-npm-run build-main",
    "build-dlls": "webpack --colors --display-error-details --config webpack/config/vendor.js",
    "build-renderer": "better-npm-run build-renderer",
    "start-main-dev": "better-npm-run start-main-dev",
    "start-renderer-dev": "better-npm-run start-renderer-dev",
    "dev": "concurrently \"npm run start-renderer-dev\" \"npm run start-main-dev\"",
    "prestart": "npm run build",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./src/main",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dll": {
      "command": "node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.dll.js --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-main": {
      "command": "node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack/webpack.config.main.prod.js --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-renderer": {
      "command": "webpack --colors --display-error-details --config=webpack/config/production.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-main-dev": {
      "command": "electron -r babel-register ./index",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK_PORT": 9000,
        "HOT": 1
      }
    },
    "start-renderer-dev": {
      "command": "node --trace-warnings -r babel-register webpack/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK_PORT": 9000,
        "HOT": 1
      }
    }
  },
  "dependencies": {
    "airplayer": "*",
    "application-config": "^1.0.1",
    "arch": "^2.1.0",
    "axios": "*",
    "better-npm-run": "^0.1.0",
    "capture-frame": "^1.0.1",
    "cheerio": "^0.22.0",
    "chromecasts": "*",
    "classnames": "^2.2.5",
    "connected-react-router": "^4.2.3",
    "cp-file": "^4.1.1",
    "crypto-js": "*",
    "crypto-random-string": "*",
    "css-loader": "^0.28.4",
    "debounce": "^1.0.0",
    "deep-equal": "^1.0.1",
    "dlnacasts": "*",
    "electron-builder": "^19.19.1",
    "event-emitter-es6": "^1.1.5",
    "fn-getter": "^1.0.0",
    "history": "^4.6.1",
    "jquery": "*",
    "limiter": "^1.1.0",
    "local-storage-es6": "^1.0.8",
    "lodash": "^4.17.4",
    "node-notifier": "^5.1.2",
    "nprogress": "^0.2.0",
    "opensubtitles-api": "^4.0.0-rc1",
    "parse-torrent": "^5.8.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.5",
    "react-redux-router": "0.0.14",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.1.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redial": "^0.5.0",
    "redux": "^3.7.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
